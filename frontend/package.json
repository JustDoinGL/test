    "@mui/material": "^6.4.3",
    "@tanstack/react-query": "^5.66.0",
    "dotenv": "^16.4.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.54.2",
    "react-router": "^7.1.5",
